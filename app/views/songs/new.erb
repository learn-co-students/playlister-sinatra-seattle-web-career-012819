<form action="/songs" method="post">
  <label for="song[name]">Song: </label>
    <input type="text" name="song[name]" id="Name">
  <label for="song[artist][name]">Artist: </label>
    <input type="text" name="song[artist][name]" id="Artist Name">
  <% Genre.all.each do |genre| %>
    <label for="song[genres][]" value="<%= genre.name %>"><%= genre.name %></label>
    <input id="<%= genre.name %>" type="checkbox" name="song[genres][]" value="<%= genre.id %>">
  <% end %>
  <button type="submit" value="Create">Create</button>
</form>