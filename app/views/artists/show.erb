<h1><%=@requested_artist.name%></h1>
<br>
<h2><%=@requested_artist.name%>'s Songs:</h2>
<ul>
  <%@artists_songs.each do |song|%>
    <li><a href="/songs/<%= song.slug %>"> <%= song.name %></a> </li>
  <%end%>
</ul>
<h2><%=@requested_artist.name%>'s Genres:</h2>
<ul>
  <%@artists_genres.each do |genre|%>
    <li><a href="/genres/<%= genre.slug %>">  <%= genre.name %></a> </li>
  <%end%>
</ul>